{
    "name": "yangweijie/ui",
    "description": "ui extension wrapper for kinbes/libui",
    "type": "library",
    "require": {
        "kingbes/libui": "dev-main"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "UI\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "authors": [
        {
            "name": "yangweijie",
            "email": "917647288@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "pestphp/pest": "*",
        "phpunit/phpunit": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "vendor/kingbes/libui"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}